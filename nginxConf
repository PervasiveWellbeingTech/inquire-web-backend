server {
    root /var/www/html/inquire;

    listen 80;
    server_name commuterweb.stanford.edu;
    location / {
        try_files $uri index.html;
    }
    location /api {
        include uwsgi_params;
        uwsgi_pass unix:/tmp/inquire.sock;
    }
}



